     <%- style %>
</head>
<body>
    <%- include('_header'); %>
<!-- 
    <% if(flash.success && flash.success.length >0 ){%>
        <h5>
            <%= flash.success%>
        </h5>
        <%}%> -->
   
    <!-- this is a body part content which is a variable -->
<main id="main-part">
    <%- body %> 
</main>

   <!-- this is end part of body variable content -->
    
   
   
   
   <%- include('_footer'); %>

    <!-- <script src="/js/script.js"></script> -->
    <%- script %>
    <script>
        <% if(flash.success && flash.success.length >0 ){%>
            new Noty({
                    theme:'relax',
                    text: '<%= flash.success %>',
                    type:'success',
                    layout:'topRight',
                    timeout:1500
                    
                }).show();
            <%}%>

            <% if(flash.error && flash.error.length >0 ){%>
            new Noty({
                    theme:'relax',
                    text: '<%= flash.error %>',
                    type:'error',
                    layout:'topRight',
                    timeout:1500
                    
                }).show();
            <%}%>
    </script>
</body>

</html>